{
  "name": "charles-b2b",
  "version": "1.0.0",
  "description": "The charles B2B hubspot theme code",
  "main": "src/js/main.js",
  "repository": "https://hello-charles.com",
  "author": "Nick-Lennart VÃ¶lker",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "watch": "concurrently \"npm run dev:webpack\" \"npm run dev:gulp\"",
    "dev:webpack": "cross-env NODE_ENV=development webpack --progress --config=./webpack.config.js --watch",
    "dev:gulp": "gulp watch",
    "lint": "npx eslint",
    "build": "npm run production",
    "production": "concurrently \"npm run prod:webpack\" \"npm run prod:gulp\"",
    "prod:webpack": "cross-env NODE_ENV=production webpack --progress --config=./webpack.config.js",
    "prod:gulp": "gulp"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "commitizen": "^3.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.12.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.1.1",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.1.0",
    "postcss-loader": "^4.0.4",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "@lottiefiles/lottie-player": "^0.5.1",
    "debounce": "^1.2.0",
    "dom-helpers": "^5.2.0",
    "mutationobserver-shim": "^0.3.7",
    "rellax": "^1.12.1",
    "scroll-events": "^1.1.3",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss": "^1.9.6",
    "throttle-debounce": "^3.0.1",
    "tiny-slider": "^2.9.3"
  }
}
